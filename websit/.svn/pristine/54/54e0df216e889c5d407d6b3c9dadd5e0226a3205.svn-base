.home-slider {
    /*width: 1920px;*/
    width: 100%;
    /*height: 920px;*/
    height: 100%;
    margin: 0 auto;
    position: relative;
}

.header-logo {
    width: 100%;
    height: 98px;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 40;
}

.header-logo img {
    width: 162px;
    height: 58px;
    margin: 20px 0 20px 46px;

}

.banner {
    width: 100%;
    /*height: 100%;*/
    /*position: absolute;
top: 0;
left: 0;*/
    box-sizing: border-box;
    border-bottom: 1px solid rgba(0, 0, 0, 0);
}

.banner .hidden {
    position: absolute;
    width: 100%;
    height: 100%;
    /*background: rgba(30, 42, 157, 0.49);*/
    background: #1e2a9d;
    opacity: 0.49;
    /*兼容ie8*/
    filter: Alpha(Opacity=49);
    z-index: 20
}

.b-btn {
    width: 24px;
    height: 48px;
    position: absolute;
    right: 0;
    left: 0;
    bottom: 60px;
    margin: 0 auto;
    z-index: 20;
    cursor: pointer;
}

.banner img {
    width: 100%;
    height: 100%;

}

